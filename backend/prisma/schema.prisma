datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Scenario {
  id         String   @id @default(cuid())
  createdAt  DateTime @default(now())
  queryRaw   String
  params     Json
  metrics    Json
  hash       String   @unique
}
