datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Scenario {
  id         String   @id @default(cuid())
  createdAt  DateTime @default(now())
  queryRaw   String
  params     Json
  metrics    Json
  hash       String   @unique
}
